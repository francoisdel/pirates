                  <h5 class="card-title"><%= boat.name %></h5>
                  <p class="card-text">Price: <%= boat.price %></p>
                  <p class="card-text">Location: <%= boat.location %></p>
                  <p class="card-text">Description: <%= boat.description %></p>
                  <p class="card-text">User Email: <%= boat.user.email %></p>
                  <div class="text-center">
                    <%= link_to "More info", boat_path(boat), class: "btn btn-primary" %>
                  </div>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      </div>
